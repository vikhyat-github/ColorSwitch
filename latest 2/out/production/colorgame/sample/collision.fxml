<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="480.0" styleClass="root" stylesheets="@end.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.collisionController">
    <children>
        <Button fx:id="okay" layoutX="59.0" layoutY="393.0" mnemonicParsing="false" onMouseClicked="#okayfunc" opacity="0.98" prefHeight="76.0" prefWidth="148.0" text="OKAY" textFill="#b9994e">
            <effect>
                <Blend />
            </effect>
            <font>
                <Font name="Arial Bold" size="24.0" />
            </font>
        </Button>
        <Label layoutX="40.0" layoutY="108.0" prefHeight="84.0" prefWidth="413.0" text="GAME OVER" textFill="WHITE">
            <font>
                <Font name="Didot" size="64.0" />
            </font>
            <effect>
                <Glow level="1.0" />
            </effect>
        </Label>
        <Button fx:id="restart" layoutX="283.0" layoutY="393.0" mnemonicParsing="false" onMouseClicked="#restartfunc" opacity="0.98" prefHeight="76.0" prefWidth="148.0" text="RESTART" textFill="#b9994e">
            <effect>
                <Blend />
            </effect>
            <font>
                <Font name="Arial Bold" size="24.0" />
            </font>
        </Button>
        <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" layoutX="138.0" layoutY="234.0" prefHeight="99.0" prefWidth="217.0" text="Label" textFill="WHITE">
            <font>
                <Font name="Chalkduster" size="72.0" />
            </font>
            <effect>
                <Glow level="0.54" />
            </effect>
        </Label>
        <Button fx:id="resurrect" layoutX="133.0" layoutY="501.0" mnemonicParsing="false" onMouseClicked="#resume" prefHeight="50.0" prefWidth="257.0" text="Resurrect : 2 stars " textFill="#b9994e">
            <font>
                <Font name="Arial Bold" size="24.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
